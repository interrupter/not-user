{
		"name": "not-user",
		"version": "0.1.7",
		"description": "not-* family user model in not- environment",
		"main": "index.js",
		"scripts": {
				"test": "./node_modules/.bin/mocha --reporter spec ./test/**.js",
				"pretest": "./node_modules/.bin/eslint ./src/**/**.js",
				"lint": "./node_modules/.bin/eslint ./src/**/**.js --fix",
				"docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
				"cover": "nyc npm test"
		},
		"repository": {
				"type": "git",
				"url": "git+https://github.com/interrupter/not-user.git"
		},
		"keywords": [
				"not-framework",
				"not-node",
				"user"
		],
		"author": "interrupter <alex.repin@mail.ru> (http://github.com/interrupter)",
		"license": "ISC",
		"bugs": {
				"url": "https://github.com/interrupter/not-user/issues"
		},
		"homepage": "https://github.com/interrupter/not-user#readme",
		"dependencies": {
				"generate-password": "^1.4.2",
				"mongoose": "^5.6.9",
				"not-config": "0.0.4",
				"not-error": "0.0.20",
				"not-filter": "^0.2.1",
				"not-locale": "*",
				"not-log": "0.0.14",
				"not-node": "^3.3.37",
				"not-one-time-code": "^0.1.1",
				"uuidv4": "*",
				"validator": "^11.1.0"
		},
		"devDependencies": {
				"chai": "^4.2.0",
				"chai-as-promised": "*",
				"eslint": "^6.1.0",
				"ink-docstrap": "*",
				"jsdoc": "^3.6.3",
				"mocha": "^6.2.0",
				"mongodb-memory-server": "^5.2.0",
				"nyc": "^13.3.0",
				"request": "^2.88.0",
				"retire": "^2.0.3",
				"should": "^13.2.1"
		},
		"nyc": {
				"include": [
						"index.js",
						"src/**/**/**.js"
				],
				"exclude": [
						"test/**/**/**/**.js"
				],
				"reporter": [
						"html"
				],
				"check-coverage": true,
				"per-file": true,
				"lines": 95,
				"statements": 95,
				"functions": 95,
				"branches": 95,
				"watermarks": {
						"lines": [
								80,
								95
						],
						"functions": [
								80,
								95
						],
						"branches": [
								80,
								95
						],
						"statements": [
								80,
								95
						]
				}
		}
}
