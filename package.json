    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-sonarjs": "^0.15.0",
    "expect": "^29.0.3",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "helmet": "^6.0.0",
    "ink-docstrap": "*",
