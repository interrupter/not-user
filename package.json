  "nyc": {
    "include": [
      "index.js",
      "src/**/**/**.js"
    ],
    "exclude": [
      "test/**/**/**/**.js"
    ],
    "reporter": [
      "html"
    ],
    "check-coverage": true,
    "per-file": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
